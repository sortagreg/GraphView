<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GraphView">
        <attr name="numberOfVerticalMarkers" format="integer"/>
        <attr name="numberOfHorizontalMarkers" format="integer"/>
        <attr name="numberOfVerticalLabels" format="integer"/>
        <attr name="numberOfHorizontalLabels" format="integer"/>
        <attr name="axisMarginTop" format="integer" />
        <attr name="axisMarginBottom" format="integer" />
        <attr name="axisMarginLeft" format="integer" />
        <attr name="axisMarginRight" format="integer" />
        <attr name="shouldDrawBox" format="boolean" />
        <attr name="labelStyle" format="integer">
            <enum name="Standard" value="0"/>
            <enum name="Unfolded" value="1"/>
            <enum name="Custom" value="2"/>
        </attr>
        <attr name="title" format="string"/>
    </declare-styleable>
</resources>